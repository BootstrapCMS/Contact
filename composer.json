{
    "name": "graham-campbell/contact",
    "description": "Contact Is A Contact Form Backend For Laravel 5",
    "keywords": ["laravel", "framework", "contact", "form", "contact form", "Contact", "Laravel Contact", "Laravel-Contact", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/contracts": "5.0.*",
        "illuminate/mail": "5.0.*",
        "illuminate/routing": "5.0.*",
        "illuminate/support": "5.0.*",
        "illuminate/validation": "5.0.*",
        "illuminate/view": "5.0.*",
        "graham-campbell/core": "~2.0",
        "graham-campbell/binput": "~3.0",
        "graham-campbell/throttle": "~3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Contact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Contact\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
